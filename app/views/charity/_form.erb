<%= form_for @charity do |f| %>
	<%= f.text_field :name, :placeholder => 'name' %>
	<%= f.text_field :category, :placeholder => 'category' %>
	<%= f.text_area :description, :placeholder => 'description' %>
	<%= f.text_field :website, :placeholder => 'website' %>
	<%= f.text_field :number, :placeholder => 'number' %>
	<%= f.text_field :donation_url, :placeholder => 'donation url' %>
	<%= f.submit %>
<% end %>